<map name="Globe" type="geocentric" version="2">

	<!--全球影像图-->
	<image name="GlobeImage" driver="gdal">
		<url>./world.tif</url>
	</image>

	<!--全球高程图-->
	<heightfield name="GlobeHeightfiled" driver="gdal">
		<url>./srtm30plus.tif</url>
	</heightfield>

	<!--文件缓存-->
	<options>
		<cache type="filesystem">
			<path>c:/osgearth_cache</path>
		</cache>
	</options>

</map>
